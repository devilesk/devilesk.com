            "data": [
                {
                    "id": [
                        "general"
                    ],
                    "name": "General",
                    "notes": [
                        {
                            "note": "Haste rune duration reduced from 30 to 25",
                            "detail": []
                        },
                        {
                            "note": "The fixed portion of the XP Hero Bounty for first 5 levels is reduced from 100\/120\/160\/220\/300 to 100\/120\/140\/160\/180 (then continues +100 per level as usual)",
                            "detail": []
                        },
                        {
                            "note": "AoE Gold Bounty, for teams that are behind, now has a small additional component that doesn't fully scale with net worth (100\/75\/50\/35\/25 for 1\/2\/3\/4\/5 heroes, scales linearly from 0 to 4k net worth difference)",
                            "detail": []
                        },
                        {
                            "note": "Small adjustments to AoE Gold (non-networth component)",
                            "detail": [
                                "Old:",
                                "1: 154 + 7.7 * Level",
                                "2: 115.5 + 6.6 * Level",
                                "3: 66 + 5.5 * Level",
                                "4: 38.5 + 4.4 * Level",
                                "5: 33 + 4.4 * Level",
                                "",
                                "New",
                                "1: 150 + 8 * Level",
                                "2: 100 + 7 * Level",
                                "3: 40 + 6 * Level",
                                "4: 25 + 4 * Level",
                                "5: 20 + 4 * Level"
                            ]
                        }
                    ]
                }
            ]
