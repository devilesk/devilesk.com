{
  "name": "devilesk.com",
  "version": "1.0.0",
  "description": "devilesk.com",
  "main": "index.js",
  "scripts": {
    "browserify": "node scripts/browserify.js",
    "sass": "node-sass src/scss/site.scss content/media/css/site.css",
    "postcss": "node scripts/postcss.js",
    "build:js": "npm run browserify",
    "build:css": "npm run sass && npm run postcss",
    "build": "npm run build:js && npm run build:css",
    "hyde:dev": "hyde gen -c site.yaml",
    "hyde:prod": "hyde gen -c site-prod.yaml",
    "assetrev": "gulp asset-rev",
    "data": "python scripts/generate.py",
    "gen:dev": "npm run data && npm run build && npm run assetrev && npm run hyde:dev",
    "gen:prod": "npm run data && npm run build && npm run assetrev && npm run hyde:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://devilesk@bitbucket.org/devilesk/devilesk.git"
  },
  "author": "devilesk",
  "license": "MIT",
  "homepage": "https://bitbucket.org/devilesk/devilesk#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "browserify": "^13.3.0",
    "factor-bundle": "^2.5.0",
    "gulp": "^3.9.1",
    "gulp-rev-all": "^0.9.7",
    "js-yaml": "^3.7.0",
    "node-sass": "^4.3.0",
    "postcss": "^5.2.10"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "dota-datafiles": "^0.3.0",
    "jquery": "^3.1.1",
    "photoswipe": "^4.1.1",
    "slick-carousel": "^1.6.0"
  }
}
